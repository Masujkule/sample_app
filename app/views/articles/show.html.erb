<div class="articles_wrapper">

<h1 class="title">
	<%= @article.title %>
</h1>

<p class="date">
	Submitted <%= time_ago_in_words(@article.created_at) %> ago
	|
</p>

<p class="body">
	<%= @article.body %>
</p>

<p class="image">
  <strong>Image:</strong>
  <%= image_tag @article.image.url %>
</p>




</div>

